{
  "name": "railroad/railnotifications",
  "description": "Laravel notification system using entities",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Caleb Favor",
      "email": "calebfavor@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^9.0",
    "guzzlehttp/guzzle": "^7.2",
    "kreait/firebase-php": "^6.3",
    "doctrine/dbal": "^3.3",
    "code-lts/laravel-fcm": "1.7.*",
    "symfony/dom-crawler": "^6.0",
    "railroad/doctrine": "2.0.*",
    "railroad/doctrine-array-hydrator": "^2.0",
    "jms/serializer": "1.*",
    "ext-redis": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "orchestra/testbench": "^7.4",
    "dms/phpunit-arraysubset-asserts": "^0.4.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/railroadmedia/laravel-apidoc-generator"
    }
  ],
  "autoload": {
    "psr-4": {
      "Railroad\\Railnotifications\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Railnotifications\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelFCM\\FCMServiceProvider"
      ],
      "aliases": {
        "FCM": "LaravelFCM\\Facades\\FCM"
      }
    }
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true,
      "composer/package-versions-deprecated": true
    }
  }
}

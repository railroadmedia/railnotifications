{
  "name": "railroad/railnotifications",
  "description": "Laravel notification system using entities",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Caleb Favor",
      "email": "calebfavor@gmail.com"
    }
  ],
  "require": {
    "php": "~7",
    "laravel/framework": "5.6.*",
    "guzzlehttp/guzzle": "~6.0",
    "kreait/firebase-php": "^4.35",
    "doctrine/dbal": "^2.5",
    "brozot/laravel-fcm": "1.3.*",
    "symfony/dom-crawler": "^5.1",
    "railroad/doctrine": "1.0.*",
    "railroad/doctrine-array-hydrator": "^1.0",
    "jms/serializer": "1.*",
    "ext-redis": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "orchestra/testbench": "3.6.*@dev",
    "orchestra/database": "3.6.*@dev"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/railroadmedia/laravel-apidoc-generator"
    }
  ],
  "autoload": {
    "psr-4": {
      "Railroad\\Railnotifications\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Railnotifications\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelFCM\\FCMServiceProvider"
      ],
      "aliases": {
        "FCM": "LaravelFCM\\Facades\\FCM"
      }
    }
  }
}
